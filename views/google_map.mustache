<!DOCTYPE html>
<link rel="stylesheet" href="../../stylesheets/google_map.css">

<link rel="stylesheet" type="text/css" href="../../stylesheets/jscal2.css" />
<link rel="stylesheet" type="text/css" href="../../stylesheets/border-radius.css" />
<link rel="stylesheet" type="text/css" href="../../stylesheets/steel.css" />

<script src="http://code.jquery.com/jquery-1.7.1.min.js"></script>	
<script type="text/javascript" src="http://maps.google.com/maps/api/js?sensor=false"></script>
<script type="text/javascript" src="../../javascripts/Fluster2.packed.js"></script>	
<script type="text/javascript" src="../../javascripts/google_map.js"></script>

<script type="text/javascript" src="../../javascripts/jscal2.js"></script>
<script src="../../javascripts/jscal2.js"></script>
<script src="../../javascripts/en.js"></script>

<div class="container" style="width: 700px">
	<div class="tagsWrapper">
		<form method="post" action="../results/google-map">
			<button  type="submit" id="updateData">Update map data</button>
			<div class="filterDiv">
				<label for="filter">Write comma-seperated tags for filter</label>
				<input id="tags" name="filter" type="text"  class="filterInputField"></input>
			</div>
		</form>
		<br />
		<button onclick="loaddata();">Refresh</button>
	</div>
	
	<div id="startdate" class="date" style="width:350px; float:left;">
		<label>From</label>
		<input size="20" id="f_date1" /><button id="f_btn1">...</button>

		<script type="text/javascript">
			Calendar.setup({
			inputField : "f_date1",
			trigger    : "f_btn1",
			onSelect   : function() { this.hide(); },
			showTime   : 12,
			dateFormat : "%Y-%m-%d %I:%M %p"
			});
		</script>
	</div>
	
	<div id="enddate" class="date" style="width:350px; float:right;">
		<button id="f_btn2" style="float:right;">...</button>
		<input size="20" id="f_date2" style="float:right;" />
		<label style="float:right;">To</label>

		<script type="text/javascript">
			Calendar.setup({
			inputField : "f_date2",
			trigger    : "f_btn2",
			onSelect   : function() { this.hide(); getenddate(); },
			showTime   : 12,
			dateFormat : "%Y-%m-%d %I:%M %p"
			});
		</script>
	</div>
	
	<div id="map_canvas" class="map" style="width: 700px; height: 500px"></div>
</div>
